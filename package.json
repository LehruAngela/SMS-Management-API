  "bugs": {
    "url": "https://github.com/LehruAngela/SMS-Management-API/issues"
  },
  "homepage": "https://github.com/LehruAngela/SMS-Management-API#readme",
  "dependencies": {
    "@babel/preset-env": "^7.5.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "babel-cli": "^6.26.0",
    "eslint": "^6.1.0",
    "mongoose": "^5.6.7",
    "nodemon": "^1.19.1",
    "rimraf": "^2.6.3"
  }
